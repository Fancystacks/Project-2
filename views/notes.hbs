{{> messages}}
<h1>My Notes</h1>
<h2>Welcome, {{email}}! You're logged in!</h2>

<div class="container">
  <div class="form">
  <div class="row">
    <div class="input-field col s6">
      <i class="material-icons prefix">textsms</i>
      <input type="text" id="autocomplete-input" class="autocomplete">
      <label for"autocomplete-input">Strain Name</label>
    </div>
    <div class="input-field col s6">
      <input id="dispensary" type="text" class="validate">
      <label for="dispensary">Dispensary Name</label>
    </div>
  </div>
  <div class="row">
    <div class="input col s6">
      <select>
        <option value="" disabled selected>Strain Species</option>
        <option value="1">Sativa</option>
        <option value="2">Indica</option>
        <option value="3">Hybrid</option>
      </select>
    </div>
    <div class="input-range col s6">
      <form action="#">
        <p class="range-field">
          <label>Potency</label>
          <input type="range" id="potency" min="0" max="40" step=".1">
        </p>
      </form>
    </div>
  </div>
  <div class="row">
    <div class="input-field col s12">
      <textarea id="notes" class="materialize-textarea"></textarea>
      <label for="notes">My Notes</label>
    </div>
  </div>
    <button class="btn waves-effect waves-light" type="submit" name="action">Submit
    <i class="material-icons right">send</i>
    </button>
  </div>
</div>
